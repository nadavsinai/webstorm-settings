<application>
  <component name="XDebuggerSettings">
    <data-views />
    <general>
      <evaluation-dialog-mode>CODE_FRAGMENT</evaluation-dialog-mode>
      <unmute-on-stop>true</unmute-on-stop>
    </general>
    <debuggers>
      <debugger id="javascript">
        <configuration watchReturnValues="true">
          <custom-object-presentation />
          <stepping-filters-enabled>false</stepping-filters-enabled>
        </configuration>
      </debugger>
    </debuggers>
  </component>
</application>